{
  "name": "v6-web-sdk-sample-integration-server",
  "version": "1.0.0",
  "private": true,
  "description": "Node.js Express server that provides several routes that wrap the PayPal APIs",
  "main": "src/server.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "check-node-version": "tsx src/checkNodeVersion.ts",
    "clean": "rm -rf dist",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "echo \"eslint is not set up\"",
    "prestart": "npm run check-node-version",
    "start": "dotenvx run -f ../../.env --quiet -- tsx --watch src/server.ts",
    "start-production": "NODE_ENV=production node dist/server.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.4",
    "@ngrok/ngrok": "^1.7.0",
    "@paypal/paypal-server-sdk": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/semver": "^7.7.1",
    "prettier": "^3.7.4",
    "semver": "^7.7.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
