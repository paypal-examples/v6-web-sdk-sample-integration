{
  "name": "v6-web-sdk-sample-integration-server",
  "version": "1.0.0",
  "private": true,
  "description": "Node.js Express server that provides several routes that wrap the PayPal APIs",
  "main": "src/server.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "check-node-version": "tsx src/checkNodeVersion.ts",
    "clean": "rm -rf dist",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest",
    "typecheck": "tsc --build --noEmit",
    "lint": "eslint . && npm run typecheck",
    "prestart": "npm run check-node-version",
    "start": "dotenvx run -f ../../.env --quiet -- tsx --watch src/server.ts",
    "start-production": "NODE_ENV=production node dist/src/server.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@paypal/paypal-server-sdk": "^2.2.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@flydotio/dockerfile": "^0.7.10",
    "@ngrok/ngrok": "^1.7.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/semver": "^7.7.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "semver": "^7.7.4",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  }
}
